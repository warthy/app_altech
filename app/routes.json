{
  "/": {
    "controller": "HomeController::index"
  },

  "/client": {
    "controller": "ClientController::index"
  },


  "/client/ticket": {
    "controller": "TicketController::index"
  },
  "/client/ticket/:id": {
    "controller": "TicketController::view",
    "requirements": {
      "id": "\\d+"
    }
  }
}